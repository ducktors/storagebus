{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "outputs": [],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "outputs": [],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "format": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "test:ci": {
      "outputs": [],
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    }
  }
}